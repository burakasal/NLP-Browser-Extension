{
  "manifest_version": 2,

  "name": "NLP Extension",
  "description": "Exchanges data with backend",
  "version": "1.4",

  "icons": {
    "48": "res/img/icon.png"
  },

  "permissions": ["tabs", "<all_urls>", "activeTab"],

  "browser_action": {
    "default_icon": "res/img/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
        "scripts": ["res/js/jquery-3.5.1.js"]
    }
}